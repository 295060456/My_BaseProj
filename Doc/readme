mansonry 高级用法
@property (nonatomic, strong) MASConstraint *Topic_headerViewHeightConstraint;

[_Topic_headerView mas_makeConstraints:^(MASConstraintMaker *make) {
make.top.left.right.equalTo(self.view);
self.Topic_headerViewHeightConstraint = make.height.mas_equalTo(0.0);
}];

self.Topic_headerView.Topic_showBlock = ^(BOOL isShow) {
if (isShow) {
CGFloat headerViewHeight = CGRectGetWidth([[UIScreen mainScreen] bounds])/5.5;
weakSelf.Topic_headerViewHeightConstraint.mas_equalTo(headerViewHeight);
weakSelf.Topic_headerView.hidden = NO;
} else {
weakSelf.Topic_headerViewHeightConstraint.mas_equalTo(0.0);
weakSelf.Topic_headerView.hidden = YES;
}
[weakSelf.Topic_headerView layoutIfNeeded];
};

